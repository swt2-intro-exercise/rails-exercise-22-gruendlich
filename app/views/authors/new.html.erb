<%= form_with url: authors_path, local: true do |form| %>
  <p>
    <%= form.label :first_name, 'First Name' %><br>
    <%= form.text_field :first_name %>
  </p>

  <p>
    <%= form.label :'Last Name' %><br>
    <%= form.text_field :'last_name' %>
  </p>

  <p>
    <%= form.label :'Homepage' %><br>
    <%= form.text_area :'homepage' %>
  </p>

  <p>
    <%= form.submit %>
  </p>
<% end %>